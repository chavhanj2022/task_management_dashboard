<!-- _login_links.html.erb -->
<ul class="navbar-nav">
  <% if !current_user%>
    <li class="nav-item">
      <%= link_to "Login", login_path, class: "nav-link" %>
    </li>
    <li class="nav-item">
      <%= link_to "Register", signup_path, class: "nav-link" %>
    </li>
  <%else%>
    <li class="nav-item">
      <%= link_to "Logout", logout_path, method: :delete ,class: "nav-link" %>
    </li>
  <%end %>
</ul>
